{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "title": "masterTemplate",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "adminusername": {
            "type": "string",
            "metadata": {
                "description": "Username for the Virtual Machine."
            }
        },
        "adminpassword": {
            "type": "string",
            "metadata": {
                "description": "Password for the Virtual Machine."
            }
        }
    },
    "variables": {
    },
    "resources": [
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2018-05-01",
            "name": "linkedTest",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "https://raw.githubusercontent.com/tadrian88/DSF-Certification-ARMSkeleton/master/Environments/Test/masterLinked.json",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": ""
            }
        }
    ],
    "outputs": {
        "OrchestratorVMUser": {
            "value": "[parameters('adminusername')]",
            "type": "string"
        },
        "OrchestratorVMPass": {
            "value": "[parameters('adminpassword')]",
            "type": "string"
        },
        "LinkedOutput":{
            "value": "[reference('linkedTest').outputs.linkedOutput.value]",
            "type": "string"
        }
    }
}
